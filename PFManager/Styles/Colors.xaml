<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundMain">#323232</Color>
    <SolidColorBrush x:Key="BackgroundMainBrush" Color="{StaticResource BackgroundMain}"/>

    <Color x:Key="BackgroundSub">#484848</Color>
    <SolidColorBrush x:Key="BackgroundSubBrush" Color="{StaticResource BackgroundSub}"/>

    <Color x:Key="ForegroundMain">#fff</Color>
    <SolidColorBrush x:Key="ForegroundMainBrush" Color="{StaticResource ForegroundMain}"/>

    <Color x:Key="ForegroundSub">#fff</Color>
    <SolidColorBrush x:Key="ForegroundSubBrush" Color="{StaticResource ForegroundSub}"/>
</ResourceDictionary>